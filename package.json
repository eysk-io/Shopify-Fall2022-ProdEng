{
  "name": "inventorify",
  "version": "1.0.0",
  "description": "Inventory tracking web application for Shopify's Production Engineer Intern Challenge",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "test": "cross-env NODE_ENV=testing jest",
    "test:resources": "cross-env NODE_ENV=testing jest -- /tests/resources/product",
    "test:crud": "cross-env NODE_ENV=testing jest -- /tests/utils/crud",
    "dev": "nodemon --exec npm run restart",
    "restart": "rimraf dist && npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyskim/Inventorify.git"
  },
  "author": "Eric Y. Kim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eyskim/Inventorify/issues"
  },
  "homepage": "https://github.com/eyskim/Inventorify#readme",
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.5",
    "morgan": "^1.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "cross-env": "^7.0.3",
    "jest": "^27.4.7",
    "mongodb-memory-server": "^8.1.0",
    "nodemon": "^2.0.15"
  }
}
